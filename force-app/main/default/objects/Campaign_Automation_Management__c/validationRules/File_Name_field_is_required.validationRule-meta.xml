<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>File_Name_field_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Campaign_Audience_Category__c, &apos;Custom Census File&apos;) , ISBLANK(File_Name__c))</errorConditionFormula>
    <errorDisplayField>File_Name__c</errorDisplayField>
    <errorMessage>File Name field is required</errorMessage>
</ValidationRule>
