.config-property-toggle .slds-checkbox_toggle [type=checkbox]+.slds-checkbox_faux_container {
    margin-left: auto;
}

lightning-input.apt-input-toggle .slds-checkbox_toggle {
    align-items: center;
    align-content: center;
}

lightning-input.apt-input-toggle .slds-checkbox_toggle .slds-checkbox_faux {
    width: 2.6875rem;
    height: 1.125rem;
}

lightning-input.apt-input-toggle .slds-checkbox_toggle [type=checkbox]:checked+.slds-checkbox_faux_container .slds-checkbox_faux {
    background-color: #0070D2;
}

lightning-input.apt-input-toggle .slds-checkbox_toggle .slds-form-element__label:empty {
    padding: 0;
}

lightning-input.apt-input-toggle .slds-checkbox_toggle [type=checkbox]:checked+.slds-checkbox_faux_container .slds-checkbox_faux:before,
lightning-input.apt-input-toggle .slds-checkbox_toggle .slds-checkbox_faux:after {
    top: 1px;
    right: 1px;
    width: 0.875rem;
    height: 0.875rem;
}

lightning-input.apt-input-toggle .slds-checkbox_toggle [type=checkbox]:checked+.slds-checkbox_faux_container .slds-checkbox_faux:after {
    top: 1px;
    left: 0.5rem;
    height: 0.625rem;
    width: 0.5rem;
}

.advance-multi-select-container .slds-checkbox__label .slds-form-element__label {
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.advance-multi-select-container .slds-checkbox .slds-checkbox__label .slds-form-element__label {
    font-size: 12px;
    color: #080707;
    line-height: 19px;
}

.custom-label-lookup lightning-input .slds-input {
    overflow: hidden;
    white-space: no-wrap;
    text-overflow: ellipsis;
}

.border-less-combobox lightning-input .slds-input {
    border: none;
}

.border-less-combobox lightning-input .slds-input:focus, .slds-input:active {
    box-shadow: none;
    outline: none;
}

.compare-checked button {
    border-color: #f3f2f2;
    background: #f3f2f2;
    color: #2B2826;
    font-size: 13px;
}

.compare-unchecked button {
    color: #2B2826;
    font-size: 13px;
}

.checkbox-group .slds-checkbox__label .slds-form-element__label {
    color: #080707;
    font-size: 12px;
    line-height: 19px;
    letter-spacing: 0.15px;
}

.checkbox-group .slds-checkbox [type=checkbox]:checked+.slds-checkbox_faux, .slds-checkbox [type=checkbox]:checked+.slds-checkbox--faux, .slds-checkbox [type=checkbox]:checked~.slds-checkbox_faux, .slds-checkbox [type=checkbox]:checked~.slds-checkbox--faux, .slds-checkbox [type=checkbox]:checked+.slds-checkbox__label .slds-checkbox_faux, .slds-checkbox [type=checkbox]:checked+.slds-checkbox__label .slds-checkbox--faux {
