}

lightning-input.apt-input-checkbox .slds-checkbox_add-button [type=checkbox][disabled]~.slds-checkbox_faux:before,
lightning-input.apt-input-checkbox .slds-checkbox_add-button [type=checkbox][disabled]+.slds-checkbox_faux:before,
lightning-input.apt-input-checkbox .slds-checkbox_add-button [type=checkbox][disabled]~.slds-checkbox_faux:after,
lightning-input.apt-input-checkbox .slds-checkbox_add-button [type=checkbox][disabled]+.slds-checkbox_faux:after {
    background-color: #FFFFFF;
}

lightning-input.apt-input-checkbox .slds-checkbox_add-button {
    height: 2rem;
}

lightning-input.apt-input-checkbox .slds-checkbox_add-button [type=checkbox]:focus+.slds-checkbox_faux {
    box-shadow: 0 0 3px #3dc376;
