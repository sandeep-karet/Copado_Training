/* Bigbox CSS */

#bigbox {
    background-color: #fff;
    margin-left: 0; /* Start position: right outside */
    -webkit-transition: margin-left 1s;
    -moz-transition: margin-left 1s;
    -o-transition: margin-left 1s;
    transition: margin-left 1s;
}
#bigbox.home_inside {
    margin-left: 0;
}

#bigbox.aptMenuOpen {
    margin-left: 50%;
}

#bigbox.about_inside {
    margin-left: -100%;
}

#bigbox.transition { /* Effects only */
    -webkit-transition: margin-left 1s;
    -moz-transition: margin-left 1s;
    -o-transition: margin-left 1s;
    transition: margin-left 1s;
}


#home_content {
    margin-left: 0; /* inside the #bigbox */
    background-color: #fff;
}


#about_content {
    margin-left: 100%; /* "relative" position to #home_content */
