}

#backIcon:before {
    color: #FFF;
    background: #3276b1;
    background: -moz-linear-gradient(-45deg, #3276b1 0%, #428bca 100%);
    background: -o-linear-gradient(-45deg, #3276b1 0%, #428bca 100%);
