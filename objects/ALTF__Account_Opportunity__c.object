    </fields>
    <fields>
        <fullName>ALTF__Opportunity_Type__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( OR(ISBLANK(ALTF__Opportunity__c), ISNULL(ALTF__Opportunity__c)) ,&apos;Potential&apos;,  IF( ALTF__Opportunity__r.IsClosed , IF( ALTF__Opportunity__r.IsWon , &apos;Won&apos;, &apos;Lost&apos;), &apos;Current&apos; ) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
