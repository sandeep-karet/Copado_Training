        <deprecated>false</deprecated>
        <formula>if(isblank(ALTF__Opportunity__c), false, if(OR(ALTF__Opportunity__r.CreatedDate &gt; CreatedDate, ALTF__Qualified_Flag__c == TRUE), true, false))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
