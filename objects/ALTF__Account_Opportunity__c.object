        <fullName>ALTF__Is_Potential__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(OR(ALTF__Opportunity__c = &apos;&apos;, ISNULL(ALTF__Opportunity__c)), 1, 0 )</formula>
