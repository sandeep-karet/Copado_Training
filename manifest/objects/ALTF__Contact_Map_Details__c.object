    <fields>
        <fullName>ALTF__Squares__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>MIN(6,
(CASE(ALTF__Status__c,
&quot;Mentor&quot;, 2,
&quot;Enemy&quot;, 2,
&quot;Supporter&quot;, 1,
&quot;Non-Supporter&quot;, 1,
&quot;Neutral&quot;, 0,
&quot;Unknown&quot;, 0,
0))
+
(CASE(ALTF__Political__c,
&quot;Inner Circle&quot;, 2,
&quot;Political Structure&quot;, 1,
&quot;Unknown&quot;, 0,
&quot;Outside Political Structure&quot;, 0,
0))
+
(CASE(ALTF__Buyer_Role__c,
&quot;decision maker&quot;, 2,
&quot;approver&quot;, 2,
&quot;evaluator&quot;, 1,
&quot;user&quot;, 0,
&quot;other&quot;, 0,
0))
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Squares</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
