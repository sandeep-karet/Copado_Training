    <validationRules>
        <fullName>ALTF__Check_Green_Amber_Range</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Green__c &lt; ALTF__Amber__c</errorConditionFormula>
        <errorMessage>Green % must be greater than Amber %</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Duration_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Duration__c &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Duration__c</errorDisplayField>
        <errorMessage>Duration must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Percentage_check_AP</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Amber__c &gt; 1 || ALTF__Amber__c &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Amber__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Percentage_check_GP</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Green__c &gt; 1 || ALTF__Green__c &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Green__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Percentage_check_QP</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Qualification__c  &gt; 1 || ALTF__Qualification__c &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Qualification__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
