    <validationRules>
        <fullName>Apttus_Approval__Next_Level_Approver_For_Level_6</fullName>
        <active>true</active>
        <description>Next Level Approver should not be specified for Level 6 users.</description>
        <errorConditionFormula>AND((ISPICKVAL(Apttus_Approval__Approval_Level__c, &apos;Level 6&apos;)) , NOT(Apttus_Approval__Next_Level_Approver__c = &quot;&quot;))</errorConditionFormula>
