    <fields>
        <fullName>ALTF__Calculated_Close_Date__c</fullName>
        <deprecated>false</deprecated>
        <formula>IF ( 
	ALTF__Opportunity__r.IsClosed  || ALTF__Is_Altify_Opp__c = false , 
	IF (
		ALTF__Opportunity__r.IsWon  || ALTF__Is_Altify_Opp__c = false, 
		ALTF__Opportunity__r.CloseDate, 
		null
	),
	IF (
			NULLVALUE (ALTF__Pause_Date__c,  DATEVALUE(NOW())) &gt; DATEVALUE(NOW()), 
			ALTF__Pause_Date__c  +  (NULLVALUE(ALTF__Weeks_To_Close__c, 0) * 7), 
			DATEVALUE(NOW())  +  (NULLVALUE(ALTF__Weeks_To_Close__c, 0) * 7)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Qualification Calculated Close Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
