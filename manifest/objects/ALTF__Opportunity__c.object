        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(OR(NOT(ALTF__Is_Actions_Ever_Planned__c), ALTF__Opportunity__r.IsClosed), 
        &apos;Blank&apos;, 
        IF(OR(ISNULL(ALTF__Min_Due_Date_All_Tasks__c), ALTF__Min_Due_Date_All_Tasks__c &gt;= TODAY()), 
            &apos;Yes&apos;, 
            IF(ALTF__Min_Due_Date_All_Tasks__c &lt; (TODAY() - 3), 
                &apos;No&apos;, 
                IF( ALTF__Min_Due_Date_All_Tasks__c &gt;= (TODAY() - 3), 
                    &apos;Unknown&apos;,
    &apos;No&apos;))))</formula>
