    <validationRules>
        <fullName>LVGO__Check_oneApp_Access_n_oneApp_start_Date</fullName>
        <active>true</active>
        <description>This is created as part of SCDEV-5393. It is to check oneApp Access and oneApp Start date both the fields has a values in it.</description>
        <errorConditionFormula>AND ($Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c,
OR
(
  AND(  
    LVGO__OneApp_Access__c= TRUE, 
    ISBLANK(LVGO__OneApp_Migration_Date__c)),

  AND( 
    LVGO__OneApp_Access__c = FALSE, 
    NOT(ISBLANK(LVGO__OneApp_Migration_Date__c))
    ) 
  )
)</errorConditionFormula>
        <errorMessage>If OneApp Access is checked you must enter a OneApp Start Date value. If you enter a OneApp Start Date value then you need to enable OneApp Access flag.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Limit_Characters</fullName>
        <active>true</active>
        <description>Only allow uppercase letters, numbers, and a dash</description>
        <errorConditionFormula>AND(NOT(REGEX(  LVGO__ClientMemberCode__c ,&quot;^[0-9 A-Z -]*&quot;)),$Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c)</errorConditionFormula>
        <errorDisplayField>LVGO__ClientMemberCode__c</errorDisplayField>
        <errorMessage>The Client Member Code must contain only UPPERCASE letters, numbers, and/or a dash</errorMessage>
    </validationRules>
