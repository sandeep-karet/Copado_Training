    <validationRules>
        <fullName>Prevent_Opportunity_Team_Member_Creation</fullName>
        <active>true</active>
        <description>Rule to prevent users to not have the ability to create new Opportunity Team Members other than System Administrator.</description>
        <errorConditionFormula>AND(
ISNEW(),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>Your assigned Profile does not allow for creating new Opportunity Team Members.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Opportunity_Team_Member_Update</fullName>
        <active>true</active>
        <description>Rule to prevent users to not have the ability to update Opportunity Team Members other than System Administrator.</description>
        <errorConditionFormula>AND(
NOT(ISNEW()),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>Your assigned Profile does not allow for updating Opportunity Team Members.</errorMessage>
    </validationRules>
</CustomObject>
