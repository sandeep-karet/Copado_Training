    <validationRules>
        <fullName>Prevent_RFP_Creation</fullName>
        <active>true</active>
        <description>Rule to prevent users to not have the ability to create new RFPs other than System Administrator.</description>
        <errorConditionFormula>AND(
ISNEW(),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>Your assigned Profile does not allow for creating new RFPs.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_RFP_Update</fullName>
        <active>true</active>
        <description>Rule to prevent users to not have the ability to update RFPs other than System Administrator.</description>
        <errorConditionFormula>AND(
NOT(ISNEW()),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>Your assigned Profile does not allow for updating RFPs.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Opp_Stage_for_RFP</fullName>
        <active>true</active>
        <description>User only allow to create opp before opp in proposal or higher</description>
        <errorConditionFormula>((Opportunity_Stage__c == &apos;Prospect&apos; || Opportunity_Stage__c 
 = &apos;Discovery&apos;) &amp;&amp; TEXT(Type__c ) == &apos;RFP&apos;)</errorConditionFormula>
        <errorMessage>Opportunity stage has to be in Proposal or higher before you can create RFP</errorMessage>
    </validationRules>
