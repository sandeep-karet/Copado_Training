    <validationRules>
        <fullName>L2O_Validate_Broadcast_Message</fullName>
        <active>true</active>
        <errorConditionFormula>if(ISCHANGED(Status) &amp;&amp; L2O_Broadcast_Message__c = true,
true,
AND(
L2O_Broadcast_Message__c = true,
 NOT(ISPICKVAL(Status, &apos;Activated&apos;)))
)</errorConditionFormula>
        <errorMessage>Broadcast Message? can only be true once the status is Activated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2O_Validate_OrderStatus</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISCHANGED(Status),
ISPICKVAL( PRIORVALUE(Status), &apos;Activated&apos;)
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>An activated Order can not go back to Draft, please Amend or process new Order Lines downstream by using the &apos;Broadcast Message?&apos; checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2O_Validate_Order_Products</fullName>
        <active>true</active>
        <errorConditionFormula>L2O_Broadcast_Message__c == true &amp;&amp; L2O_of_Activated_Hold_Order_Products__c &gt; 0</errorConditionFormula>
        <errorMessage>Order Products cannot be on Hold and Activated at the same time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Cannot_Contract_New_Order</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(), SBQQ__Contracted__c)</errorConditionFormula>
        <errorDisplayField>SBQQ__Contracted__c</errorDisplayField>
        <errorMessage>Can&apos;t contract the order. Activate the order and try again.</errorMessage>
    </validationRules>
