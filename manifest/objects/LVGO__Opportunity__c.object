 $Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c,
 ISPICKVAL( LVGO__Forecast_Manager_Call__c , &apos;Forecast - Closed&apos;),
 NOT(ISPICKVAL( LVGO__StageName__c , &apos;Closed Won&apos;)),
