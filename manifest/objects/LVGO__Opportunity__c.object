    <validationRules>
        <fullName>LVGO__Account_Name_Change_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c, ISCHANGED( LVGO__Account__c ),  $Profile.Name &lt;&gt; &apos;System Administrator&apos;)</errorConditionFormula>
        <errorDisplayField>LVGO__Account__c</errorDisplayField>
        <errorMessage>Only System Administrator can update the Account Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Benefits_Consultant_Contact_Role</fullName>
        <active>true</active>
        <description>If a Benefits Consultants was involved in the deal, you must add a Contact Role with Type - Broker before you can save this record.</description>
        <errorConditionFormula>$Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c &amp;&amp;
(LVGO__Broker_Count__c = 0.0
|| ISBLANK(LVGO__Broker_Count__c))
&amp;&amp; (
ISPICKVAL(LVGO__Benefit_Consultant_Involved_in_Deal__c, &quot;Consultant introduced us, but was not involved in selling&quot;)
|| ISPICKVAL(LVGO__Benefit_Consultant_Involved_in_Deal__c, &quot;Consultant introduced us, and was involved in selling&quot;)
|| ISPICKVAL(LVGO__Benefit_Consultant_Involved_in_Deal__c, &quot;Consultant did not introduce us, but was involved in selling&quot;)
)
&amp;&amp; NOT(ISPICKVAL(LVGO__Type__c, &quot;Renewal&quot;))
&amp;&amp; LVGO__Bypass_Contact_Role_Rules__c = FALSE
&amp;&amp; LVGO__Probability__c &gt; 0.30
&amp;&amp; NOT (ISCHANGED( LVGO__Supporting_PBM_Executive_Count__c ))
&amp;&amp;
NOT( ISPICKVAL( LVGO__Source__c , &apos;TD&apos;) )</errorConditionFormula>
        <errorMessage>If a Benefits Consultants was involved in the deal, you must first add a Contact Role with Type - Broker before you can save this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__CVS_Proposal_Reason</fullName>
        <active>true</active>
        <description>Any opportunity that is a Current CVS TDC / Aetna Client, must have a Client Proposal Preference filled out before the record could be saved.</description>
        <errorConditionFormula>$Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c &amp;&amp;
NOT(ISPICKVAL(LVGO__Type__c, &apos;Renewal&apos;)) &amp;&amp; INCLUDES(LVGO__Account__r.LVGO__Account_Grouping__c, &quot;Current CVS TDC/Aetna Clients&quot; )
	&amp;&amp;  (ISPICKVAL(LVGO__StageName__c, &quot;Proposal&quot;) || ISPICKVAL(LVGO__StageName__c, &quot;Review/Negotiation&quot;) || ISPICKVAL(LVGO__StageName__c, &quot;Contracting&quot;))
	&amp;&amp; ISBLANK( TEXT( LVGO__Client_Proposal_Preference__c )) &amp;&amp; NOT(ISPICKVAL(LVGO__Source__c, &quot;TD&quot;))</errorConditionFormula>
        <errorMessage>Any opportunity that is a Current CVS TDC / Aetna Client, must have a Client Proposal Preference filled out before the record could be saved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__CVS_Proposal_Reason_FALSE</fullName>
        <active>true</active>
        <description>Any opportunity that is  not a Current CVS TDC / Aetna Client, must leave the Client Proposal Preference blank before being able to save.</description>
        <errorConditionFormula>$Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c &amp;&amp;
NOT(INCLUDES(LVGO__Account__r.LVGO__Account_Grouping__c, &quot;Current CVS TDC/Aetna Clients&quot; ))
&amp;&amp; NOT(ISBLANK( TEXT( LVGO__Client_Proposal_Preference__c )))
&amp;&amp; NOT(ISPICKVAL(LVGO__Source__c, &quot;TD&quot;))</errorConditionFormula>
        <errorMessage>Any opportunity that is  not a Current CVS TDC / Aetna Client, must leave the Client Proposal Preference blank before being able to save.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Cannot_Closed_Won_Contract_Path_Null</fullName>
        <active>true</active>
        <description>The Contract Path must be populated before an Opportunity can be Closed Won.</description>
        <errorConditionFormula>$Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c
&amp;&amp; ISCHANGED(LVGO__StageName__c)
&amp;&amp; ISPICKVAL(LVGO__StageName__c, &quot;Closed Won&quot;)
&amp;&amp; ISBLANK( LVGO__Contract_Path__c )</errorConditionFormula>
        <errorDisplayField>LVGO__Contract_Path__c</errorDisplayField>
        <errorMessage>The Contract Path must be populated before an Opportunity can be Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Cannot_RequestCIM_for_ContractPathChange</fullName>
        <active>false</active>
        <errorConditionFormula>AND($Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c,ISPICKVAL(LVGO__Type__c,&quot;Contract Path Change&quot;),
