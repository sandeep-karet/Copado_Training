    <validationRules>
        <fullName>LVGO__Cannot_move_Stage_to_Closed_won_TD</fullName>
        <active>false</active>
        <errorConditionFormula>AND($Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c,ISPICKVAL(LVGO__Source__c, &quot;TD&quot;),
							LVGO__Product_Count__c = 0, 
							LVGO__Bypass_Product_Requirement__c = FALSE,
							ISPICKVAL( LVGO__StageName__c, &quot;Closed Won&quot;))</errorConditionFormula>
        <errorMessage>You cannot move the Stage to Closed Won for Opportunities created from TD org without adding Programs first. If you believe this is in error, please contact your System Administrator.</errorMessage>
    </validationRules>
