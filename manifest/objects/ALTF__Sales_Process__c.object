    <validationRules>
        <fullName>ALTF__Closure_Duration_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Closure_Duration__c &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Closure_Duration__c</errorDisplayField>
        <errorMessage>Closure duration must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Closure_duration_length</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Closure_Duration__c  &gt; 1000</errorConditionFormula>
        <errorDisplayField>ALTF__Closure_Duration__c</errorDisplayField>
        <errorMessage>Closure Duration cannot be greater than 1000 days</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Disabled_Default_Block</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Disabled__c = true &amp;&amp; ALTF__Default_Process__c = true</errorConditionFormula>
        <errorDisplayField>ALTF__Disabled__c</errorDisplayField>
        <errorMessage>You cannot have a disabled default process</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Percentage_check_FSP</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Final_Stage_Probability__c &gt; 1 ||  ALTF__Final_Stage_Probability__c &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Final_Stage_Probability__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Percentage_check_PP</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Pebble_Percentage__c  &gt; 1 || ALTF__Pebble_Percentage__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Pebble_Percentage__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Percentage_check_PT</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Threshold__c  &gt; 1 || ALTF__Threshold__c &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Threshold__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ALTF__Percentage_check_RP</fullName>
        <active>true</active>
        <errorConditionFormula>ALTF__Rock_Percentage__c &gt; 1 || ALTF__Rock_Percentage__c &lt; 0</errorConditionFormula>
        <errorDisplayField>ALTF__Rock_Percentage__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
