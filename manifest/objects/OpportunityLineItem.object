        <formula>PricebookEntry.Product2.Name</formula>
        <label>Product</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Quantity</fullName>
    </fields>
    <fields>
        <fullName>Revenue_Synergy_Flag__c</fullName>
        <externalId>false</externalId>
        <label>Product Revenue Synergy Flag</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Revenue_for_One_time__c</fullName>
        <formula>IF(ISPICKVAL(SBQQ__QuoteLine__r.SBQQ__SubscriptionPricing__c,&quot;&quot;),SBQQ__QuoteLine__r.SBQQ__NetTotal__c,0.00)+ SBQQ__QuoteLine__r.Up_Front_Fee_Total__c</formula>
        <label>Est. One Time Revenue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>SBQQ__ParentID__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>ID of the opportunity product that came from this product&apos;s bundle.</inlineHelpText>
        <label>Parent ID</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SBQQ__QuoteLine__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <deprecated>false</deprecated>
        <description>Quote line that generated this Opportunity Product.</description>
        <label>Quote Line</label>
        <referenceTo>SBQQ__QuoteLine__c</referenceTo>
        <relationshipLabel>Opportunity Product</relationshipLabel>
        <relationshipName>OpportunityProducts</relationshipName>
        <required>false</required>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>SBQQ__SubscriptionType__c</fullName>
        <deprecated>false</deprecated>
        <description>Indicates the subscription type to determine contract&apos;s pricing.</description>
        <inlineHelpText>Indicates the subscription type to determine contract&apos;s pricing.</inlineHelpText>
        <label>Subscription Type</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>SBQQ__SubscriptionType</valueSetName>
        </valueSet>
    </fields>
    <fields>
        <fullName>ServiceDate</fullName>
    </fields>
    <fields>
        <fullName>Source_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>true</externalId>
        <label>Source Id</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Subtotal</fullName>
    </fields>
    <fields>
        <fullName>TotalPrice</fullName>
    </fields>
    <fields>
        <fullName>UnitPrice</fullName>
    </fields>
    <fields>
        <fullName>Up_Front_Fee__c</fullName>
        <formula>SBQQ__QuoteLine__r.Up_Front_Fee__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Up Front Fee</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Vendor_Partner__c</fullName>
        <label>Vendor Partner</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>Vendor_Partner</valueSetName>
        </valueSet>
    </fields>
    <fields>
        <fullName>tinderbox__Include_Content__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If checked it will include the associated TinderBox content for this product.</description>
        <label>Include Content</label>
        <type>Checkbox</type>
    </fields>
    <validationRules>
        <fullName>Close_Date_Same_as_Product_Service_Date</fullName>
        <active>false</active>
        <description>Assures that the Opportunity Close Date is the same as the Product Date (Service Date)</description>
        <errorConditionFormula>ServiceDate&lt;&gt;Opportunity.CloseDate</errorConditionFormula>
        <errorDisplayField>ServiceDate</errorDisplayField>
        <errorMessage>The Date of this Product is not the same as the Effective Revenue Date.  Please make these Dates the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Opportunity_Product_Creation</fullName>
        <active>true</active>
        <description>Rule to prevent users to not have the ability to create new Opportunity Products other than System Administrator.</description>
        <errorConditionFormula>AND(
ISNEW(),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>Your assigned Profile does not allow for creating new Opportunity Products.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Opportunity_Product_Update</fullName>
        <active>true</active>
        <description>Rule to prevent users to not have the ability to update Opportunity Products other than System Administrator.</description>
        <errorConditionFormula>AND(
NOT(ISNEW()),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>Your assigned Profile does not allow for updating Opportunity Products.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_cannot_exceed_Number_of_Lives</fullName>
        <active>false</active>
        <description>Assures that the Product Quantity cannot exceed Number of Lives on the Opportunity</description>
        <errorConditionFormula>AND(
  Quantity&gt;Opportunity.Initial_of_Lives__c,
  Opportunity.CPQ_Pricebook__c &lt;&gt; &apos;HHS&apos;
)</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>Your Product Quantity exceeds the Number of Lives on the Opportunity.  Please correct this issue.</errorMessage>
    </validationRules>
</CustomObject>
