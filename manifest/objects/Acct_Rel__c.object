    <validationRules>
        <fullName>Broker_Fee_Pct_CANNOT_be_blank</fullName>
        <active>false</active>
        <errorConditionFormula>AND( (ISPICKVAL( Relationship_Type__c , &apos;BROKER&apos;)),
AND( isblank( Broker_Flat_Rate__c ), isblank( Broker_Percentage__c ))
)</errorConditionFormula>
        <errorMessage>Broker flat fee commission OR Broker commission percentage must be filled in for  BROKERS</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Broker_related_field_must_be_broker</fullName>
        <active>false</active>
        <description>Relationship type must be broker in order to update broker related fields</description>
        <errorConditionFormula>AND(
NOT(ISPICKVAL(Relationship_Type__c,&quot;Broker&quot;)),
OR(NOT(ISBLANK(Broker_Contact__c )),
NOT(ISBLANK(Broker_Flat_Rate__c )),
NOT(ISBLANK(Broker_Percentage__c )),
NOT(ISBLANK(Broker_Location__c ))))</errorConditionFormula>
        <errorMessage>Relationship Type must be Broker in order to input Broker Information fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_Fee_or_Percentage</fullName>
        <active>false</active>
        <errorConditionFormula>AND( OR(ISPICKVAL( Relationship_Type__c , &apos;BROKER&apos;),ISPICKVAL( Relationship_Type__c , &apos;Consultant&apos;),ISPICKVAL( Relationship_Type__c , &apos;Referral Partner&apos;))  ,

AND( !isblank( Broker_Flat_Rate__c ), !isblank( Broker_Percentage__c ))

)</errorConditionFormula>
        <errorMessage>Only one of the broker commission rate fields need to be filled in, not both</errorMessage>
    </validationRules>
