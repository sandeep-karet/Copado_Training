    <fields>
        <fullName>Contact__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>MSU Contact to have marketing hub access for this member group</description>
        <inlineHelpText>MSU Contact to have marketing hub access for this member group</inlineHelpText>
        <label>Contact</label>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>Mkt Site Users</relationshipLabel>
        <relationshipName>Mkt_Site_Users</relationshipName>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Mbr_Group__c</fullName>
        <description>SF record name of member group</description>
        <inlineHelpText>SF record name of member group</inlineHelpText>
        <label>Member Group</label>
        <referenceTo>Mbr_Group__c</referenceTo>
        <relationshipLabel>Mkt Site Users</relationshipLabel>
        <relationshipName>Mkt_Site_Users</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <label>Mkt Site User</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Mbr_Group__c</columns>
        <columns>Contact__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <displayFormat>MSU-{00000000}</displayFormat>
        <label>Mkt Site User Id</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Mkt Site Users</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Already_Has_Access</fullName>
        <active>true</active>
        <description>Prevents the creation of an MSU on an account that matches the contact&apos;s</description>
        <errorConditionFormula>Contact__r.AccountId =  Mbr_Group__r.Client_Account__r.Id</errorConditionFormula>
        <errorMessage>Contact already has access to this group based on their account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_not_Marketing_Site_User</fullName>
        <active>true</active>
        <errorConditionFormula>!Contact__r.Marketing_Site_User__c</errorConditionFormula>
        <errorMessage>Contact is not an active marketing site user</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Group</fullName>
        <active>true</active>
        <description>Member group must be Active or Pending.</description>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Mbr_Group__r.Status__c, &apos;ACTIVE&apos;)),
			NOT(ISPICKVAL(Mbr_Group__r.Status__c, &apos;PENDING&apos;)))</errorConditionFormula>
        <errorMessage>Member group must be Active or Pending.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
