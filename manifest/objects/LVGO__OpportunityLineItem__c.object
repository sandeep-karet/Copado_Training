    <validationRules>
        <fullName>LVGO__Cannot_Change_Price_without_Structure</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
            ISCHANGED( LVGO__UnitPrice__c ),
            ISBLANK( LVGO__Pricing_Structure__c ),
            $Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c
        )</errorConditionFormula>
        <errorMessage>Please add a Pricing Structure to update the sales price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Cannot_Edit_TD_Opportunity_Program</fullName>
        <active>true</active>
        <description>Cannot edit TD Opportunity Programs.</description>
        <errorConditionFormula>AND(ISPICKVAL(LVGO__Opportunity__r.LVGO__Source__c,&apos;TD&apos;),
                    NOT($Permission.LVGO__Edit_TD_Opportunity_Program),
                    $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
                    OR(ISCHANGED(LVGO__Product_Participants__c),
                    ISCHANGED(LVGO__UnitPrice__c),
                    ISCHANGED(LVGO__Quantity__c ),
                    ISCHANGED(LVGO__Pricing_Structure__c),
                    ISCHANGED(LVGO__Estimated_Partner_Admin_Fees__c),
                    ISCHANGED(LVGO__Estimated_At_Risk_PPPM__c),
                    ISCHANGED(LVGO__Up_Front_Per_Member__c)),
                    $Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c)</errorConditionFormula>
        <errorMessage>You are not allowed to update TD Opportunity Program in LVN, please make the update in TD Instance</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Disable_BH_Coaching_Check</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
                    LVGO__Disable_BH_Coaching__c,
                    NOT( CONTAINS(LVGO__Product2__r.Name, &apos;Behavioral Health&apos;)) ,
                    $Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c
        )</errorConditionFormula>
        <errorMessage>This check can be checked once Behavioral Health is added.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Disable_Teletherapy_Check</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
                    LVGO__Disable_Teletherapy__c,
                    LVGO__Product2__r.Name != &apos;myStrength Complete&apos;,
                    LVGO__Product2__r.Name != &apos;Livongo for Behavioral Health by myStrength 2.0&apos;,
                    NOT(CONTAINS(LVGO__Product2__r.Name, &apos;Solution&apos;)),
                    $Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c
        )</errorConditionFormula>
        <errorMessage>This check box can be checked only for Whole Person Solution Programs, myStrength Complete and Livongo for Behavioral Health by myStrength 2.0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Initial_Price_must_be_0</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
            ISNEW(),
            LVGO__UnitPrice__c != 0,
            $Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c
            )</errorConditionFormula>
        <errorMessage>Initial Sales Price must be 0. Please use the lookup &quot;Pricing Structure&quot; to alter this Sales price. Prices can be updated after creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__PMPM_only_for_BH_Product</fullName>
        <active>true</active>
        <description>PMPM rate can only be edited for BH 2.0 product</description>
        <errorConditionFormula>AND($Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c,ISCHANGED(LVGO__PMPM__c), 
            NOT(ISPICKVAL(LVGO__Product2__r.Family, &apos;Behavioral Health&apos;)))</errorConditionFormula>
        <errorDisplayField>LVGO__PMPM__c</errorDisplayField>
        <errorMessage>PMPM should only be entered for Behavioral Health products</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__WP_Programs_Pricing_Must_be_in_Range</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c,NOT(LVGO__Bypass_Sales_Price_Validation__c), !ISNEW(), ISCHANGED(LVGO__UnitPrice__c), LVGO__Opportunity_Type__c != &apos;Renewal&apos;,
            OR(
            AND(
            LVGO__Product2__r.Name = &apos;DM Solution&apos;,
            IF( LVGO__UnitPrice__c &gt; 107, TRUE,IF(LVGO__UnitPrice__c &lt; 76, TRUE, IF(LVGO__UnitPrice__c &lt; 95, IF(LVGO__UnitPrice__c &gt; 85, TRUE, FALSE), FALSE)))
            ),
            AND(
            LVGO__Product2__r.Name = &apos;CV Solution&apos;,
            IF(LVGO__UnitPrice__c &lt; 55, TRUE, IF(LVGO__UnitPrice__c &gt; 85, TRUE, IF(LVGO__UnitPrice__c &gt; 60, IF(LVGO__UnitPrice__c &lt; 76, TRUE, FALSE),FALSE)))
            ),
            AND(
            LVGO__Product2__r.Name = &apos;DP Solution&apos;,
            IF(LVGO__UnitPrice__c &lt; 73, TRUE, IF(LVGO__UnitPrice__c &gt; 85, TRUE, FALSE))
            )
            )
            )</errorConditionFormula>
        <errorDisplayField>LVGO__UnitPrice__c</errorDisplayField>
        <errorMessage>You&apos;re attempting to update the Sales Price outside of the defined range for this program.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
