    <fields>
        <fullName>LVGO__Gross_Program_ARR__c</fullName>
        <deprecated>false</deprecated>
        <description>THIS SHOULD NOT BE USED</description>
        <formula>CASE(
    LVGO__Program_Family__c,

    &apos;Diabetes&apos;, IF(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), (0.25 * LVGO__Product_Participants__c * (LVGO__UnitPrice__c * 12)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12,
                (0.20 * LVGO__Product_Participants__c * (LVGO__UnitPrice__c * 12)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12),
    
    &apos;Hypertension&apos;, IF(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), (0.10 * LVGO__Product_Participants__c * (LVGO__UnitPrice__c * 12)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12,
                    (0.08 * LVGO__Product_Participants__c * (LVGO__UnitPrice__c * 12)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12),

    &apos;Weight Management&apos;, IF(AND(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), LVGO__UnitPrice__c == 1.00), 608 * LVGO__Product_Participants__c * 0.10,
                         IF(LVGO__UnitPrice__c == 1.00, 608 * LVGO__Product_Participants__c * 0.08,
                         IF(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), 0.10 * LVGO__Product_Participants__c * LVGO__UnitPrice__c * 12 + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12,
                         0.08 * LVGO__Product_Participants__c * LVGO__UnitPrice__c * 12 + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12))),
    
    &apos;Prediabetes&apos;, IF(AND(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), LVGO__UnitPrice__c == 1.00), 608 * LVGO__Product_Participants__c * 0.10,
                    IF(LVGO__UnitPrice__c == 1.00, 608 * LVGO__Product_Participants__c * 0.08,
                    IF(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), 0.10 * LVGO__Product_Participants__c * LVGO__UnitPrice__c * 12 + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12,
                    0.08 * LVGO__Product_Participants__c * LVGO__UnitPrice__c * 12 + LVGO__Currently_Enrolled_Members__c* LVGO__Incremental_Sales_Price__c * 12))),

    LVGO__Product_Participants__c * LVGO__UnitPrice__c * 12 + LVGO__Product_Participants__c * 0.02 * LVGO__PMPM__c * 6
)</formula>
