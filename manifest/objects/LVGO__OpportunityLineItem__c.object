    <fields>
        <fullName>LVGO__Program_ARR__c</fullName>
        <deprecated>false</deprecated>
        <formula>CASE(
LVGO__Program_Family__c,

&apos;Diabetes&apos;, IF(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), (0.25 * LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12,
(0.20 * LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12),

&apos;Hypertension&apos;, IF(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), (0.10 * LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12,
(0.08 * LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12),

&apos;Weight Management&apos;, IF(AND(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), LVGO__UnitPrice__c == 1.00), 608 * LVGO__Product_Participants__c * 0.10,
IF(LVGO__UnitPrice__c == 1.00, 608 * LVGO__Product_Participants__c * 0.08,
IF(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), (0.10 * LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12,
(0.08 * LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12))),

&apos;Prediabetes&apos;, IF(AND(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), LVGO__UnitPrice__c == 1.00), 608 * LVGO__Product_Participants__c * 0.10,
IF(LVGO__UnitPrice__c == 1.00, 608 * LVGO__Product_Participants__c * 0.08,
IF(CONTAINS(LVGO__Opportunity__r.LVGO__Opp_Owner_Role__c, &apos;Commercial&apos;), (0.10 * LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12,
(0.08 * LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Currently_Enrolled_Members__c * LVGO__Incremental_Sales_Price__c * 12))),

(LVGO__Product_Participants__c * ((LVGO__UnitPrice__c - (LVGO__Estimated_At_Risk_PPPM__c + LVGO__Estimated_Partner_Admin_Fees__c)) * 12 + LVGO__Up_Front_Per_Member__c)) + LVGO__Product_Participants__c * 0.02 * LVGO__PMPM__c * 6
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Net Program ARR</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
