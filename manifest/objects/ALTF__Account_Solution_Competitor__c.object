        <fullName>ALTF__Wallet_Perc__c</fullName>
        <deprecated>false</deprecated>
        <formula>IF($Setup.ALTF__Account_Manager_Settings__c.ALTF__Use_Quantity_In_Market_Landscape__c,
IF (ISNULL(ALTF__Account_Solution__r.ALTF__Wallet_Quantity__c),
     NULL,
     IF (ALTF__Account_Solution__r.ALTF__Wallet_Quantity__c == 0, 
             0,  
             ALTF__Wallet_Quantity__c / ALTF__Account_Solution__r.ALTF__Wallet_Quantity__c
     )
)
,
IF (ISNULL(ALTF__Account_Solution__r.ALTF__Wallet_Size__c),
     NULL,
     IF (ALTF__Account_Solution__r.ALTF__Wallet_Size__c == 0, 
             0,  
             ALTF__Wallet_Value__c / ALTF__Account_Solution__r.ALTF__Wallet_Size__c
     )
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
