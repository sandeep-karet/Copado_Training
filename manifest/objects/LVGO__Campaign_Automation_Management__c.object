    <validationRules>
        <fullName>LVGO__Answer1_Response_Is_Required</fullName>
        <active>true</active>
        <description>Answer1 Response is required if the Answer1 Action is Email</description>
        <errorConditionFormula>AND(ISPICKVAL(LVGO__Answer1_Action__c, &apos;Send Email&apos;),
                    ISBLANK(LVGO__Answer1_Response__c),$Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c)</errorConditionFormula>
        <errorMessage>Answer1 Response is required if the Answer1 Action is &quot;Send Email&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__End_Date_cannot_be_less_the_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>LVGO__End_Date__c &lt;= LVGO__Start_Date__c</errorConditionFormula>
        <errorMessage>End Date should be greater than the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__File_Name_field_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL(LVGO__Campaign_Audience_Category__c, &apos;Custom Census File&apos;) , ISBLANK(LVGO__File_Name__c))</errorConditionFormula>
        <errorDisplayField>LVGO__File_Name__c</errorDisplayField>
        <errorMessage>File Name field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Second_round_meter_message_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND (LVGO__Enable_Second_Round_Campaign__c, OR (ISNULL(LVGO__Second_Round_Campaign_Days_Delay__c), ISNULL(LVGO__Second_Round_Campaign_Start_Date__c)))</errorConditionFormula>
        <errorMessage>&quot;Second round meter message days&quot;  and  &quot;Second Round Campaign Start Date&quot; field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Start_Date_cannot_be_in_the_past</fullName>
        <active>false</active>
        <description>Start Date cannot be in the past.</description>
        <errorConditionFormula>AND(OR(ISNEW(),
ISCHANGED(LVGO__Start_Date__c)),
NOT(ISBLANK(LVGO__Start_Date__c)),
LVGO__Start_Date__c &lt; TODAY())</errorConditionFormula>
        <errorMessage>Start Date cannot be in the past, please select a valid date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LVGO__Validation_for_Recurring_Frequency</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
     OR(
        AND(
            LVGO__Allow_Recurring_Order__c = TRUE,
            ISBLANK(TEXT(LVGO__Recurring_Order_Frequency__c))
        ),
        AND(
            NOT(ISBLANK(TEXT(LVGO__Recurring_Order_Frequency__c))),
            LVGO__Allow_Recurring_Order__c = FALSE
        )
     ),
     $Setup.LVGO__LVGO_Automation_Switch__c.LVGO__Validations__c
)</errorConditionFormula>
        <errorMessage>When Allow Recurring Order is checked, Recurring Frequency value is required.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
