        <externalId>false</externalId>
        <formula>IF (ISPICKVAL(SBQQ__BillingFrequency__c, &quot;Monthly&quot;), SBQQ__SubscriptionTerm__c / 1,
IF( (ISPICKVAL(SBQQ__BillingFrequency__c, &quot;Quarterly&quot;)), SBQQ__SubscriptionTerm__c / 12 * 4,
IF( (ISPICKVAL(SBQQ__BillingFrequency__c, &quot;Semiannual&quot;)), SBQQ__SubscriptionTerm__c / 12 * 2,
IF( (ISPICKVAL(SBQQ__BillingFrequency__c, &quot;Annual&quot;)), SBQQ__SubscriptionTerm__c / 12,
IF( (ISPICKVAL(SBQQ__BillingFrequency__c, &quot;Invoice Plan&quot;)) , NULL,
