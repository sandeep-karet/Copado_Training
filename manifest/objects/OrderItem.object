        <externalId>false</externalId>
        <formula>IF(SBQQ__Activated__c = True,

Shipping_Address_Text__Street__s + &quot;,&quot; + &quot; &quot; +
Shipping_Address_Text__City__s + &quot;,&quot; + &quot; &quot; +
TEXT(Shipping_Address_Text__StateCode__s) +  &quot;,&quot; + &quot; &quot; +
Shipping_Address_Text__PostalCode__s + &quot;,&quot; + &quot; &quot; +
TEXT(Shipping_Address_Text__CountryCode__s),

Shipping_Contact__r.MailingStreet + &quot;,&quot; + &quot; &quot; +
Shipping_Contact__r.MailingCity + &quot;,&quot; + &quot; &quot; +
Shipping_Contact__r.MailingState + &quot; &quot; +
Shipping_Contact__r.MailingPostalCode + &quot;,&quot; + &quot; &quot; +
Shipping_Contact__r.MailingCountry )</formula>
        <label>Shipping Address Text</label>
