    <validationRules>
        <fullName>SBQQ__Event_Evaluation_Pricing_Guidance</fullName>
        <active>true</active>
        <description>Prevents users from creating Price Actions related to Pricing Guidance if the evaluation event is not After Calculate.</description>
        <errorConditionFormula>ISPICKVAL(SBQQ__TargetObject__c, &apos;Quote Line Pricing Guidance&apos;) &amp;&amp; (INCLUDES(SBQQ__Rule__r.SBQQ__EvaluationEvent__c, &apos;On Initialization&apos;) || INCLUDES(SBQQ__Rule__r.SBQQ__EvaluationEvent__c, &apos;Before Calculate&apos;) || INCLUDES(SBQQ__Rule__r.SBQQ__EvaluationEvent__c, &apos;On Calculate&apos;))</errorConditionFormula>
        <errorDisplayField>SBQQ__TargetObject__c</errorDisplayField>
        <errorMessage>To use Quote Line Pricing Guidance as the Target Object, select After Calculate from the Calculator Evaluation Event dropdown list.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SBQQ__Target_Object_Validation</fullName>
        <active>true</active>
        <description>Prevents users from creating Price Actions targeting an out-of-scope object.</description>
        <errorConditionFormula>(ISPICKVAL(SBQQ__TargetObject__c, &apos;Product Option&apos;) &amp;&amp; NOT(ISPICKVAL(SBQQ__Rule__r.SBQQ__TargetObject__c, &apos;Product Option&apos;) || ISPICKVAL(SBQQ__Rule__r.SBQQ__TargetObject__c, &apos;Configurator&apos;))) || ((ISPICKVAL(SBQQ__TargetObject__c, &apos;Quote&apos;) || ISPICKVAL(SBQQ__TargetObject__c, &apos;Quote Line&apos;) || ISPICKVAL(SBQQ__TargetObject__c, &apos;Quote Line Pricing Guidance&apos;)) &amp;&amp; NOT(ISPICKVAL(SBQQ__Rule__r.SBQQ__TargetObject__c, &apos;Quote Line&apos;) || ISPICKVAL(SBQQ__Rule__r.SBQQ__TargetObject__c, &apos;Calculator&apos;)))</errorConditionFormula>
        <errorDisplayField>SBQQ__TargetObject__c</errorDisplayField>
        <errorMessage>Price Action Target Object does not match Evaluation Scope</errorMessage>
    </validationRules>
