    <validationRules>
        <fullName>Prevent_Opportunity_Split_Creation</fullName>
        <active>true</active>
        <description>Rule to prevent users to not have the ability to create new Opportunity Splits other than System Administrator.</description>
        <errorConditionFormula>AND(
ISNEW(),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>Your assigned Profile does not allow for creating new Opportunity Splits.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Opportunity_Split_Update</fullName>
        <active>true</active>
        <description>Rule to prevent users to not have the ability to update Agreements other than System Administrator.</description>
        <errorConditionFormula>AND(
NOT(ISNEW()),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>Your assigned Profile does not allow for updating Opportunity Splits.</errorMessage>
    </validationRules>
