<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Gives assigned users View access to dashboards owned by people under them in the role hierarchy.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>System - View My Teams Dashboards</label>
    <userPermissions>
        <enabled>true</enabled>
        <name>ViewMyTeamsDashboards</name>
    </userPermissions>
</PermissionSet>