        <formula>NOT(ISBLANK(SBQQ__AmendedContract__c) )</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>CPQU__CPQU - Closed Won %3D Contracted</fullName>
        <actions>
            <name>CPQU__Contracted_True</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>$Setup.CPQU__Settings__c.CPQU__Auto_Generate_Contract__c &amp;&amp;  NOT(ISBLANK(SBQQ__PrimaryQuote__c)) &amp;&amp;  CONTAINS($Setup.CPQU__Settings__c.CPQU__Auto_Generate_Contract_Opp_Stage__c, TEXT(StageName) &amp; &quot;,&quot;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>DNA - Override Future Close Date Reset</fullName>
        <actions>
            <name>Reset_Close_Date_to_Rev_Effective_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Won</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.CloseDate</field>
            <operation>equals</operation>
            <value>TODAY</value>
        </criteriaItems>
        <description>Salesforce native functionality resets a Close Date that&apos;s in the future to today&apos;s date on Closed Won.  That doesn&apos;t work for Teladoc with Close Date used as Revenue Effective Date.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
