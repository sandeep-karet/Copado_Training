    <decisions>
        <description>Determine whether or not to update the Program Overview Status to &quot;Activated&quot;</description>
        <name>Check_if_Program_Overview_should_be_Activated</name>
        <label>Check if Program Overview should be Activated</label>
        <locationX>1070</locationX>
        <locationY>1401</locationY>
        <defaultConnector>
            <targetReference>Loop_Thru_Realted_Program_Overviews</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Update_Status_to_Activate_or_Reactivate</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>currentProgramOverview.LVGO__Program_Start_Date__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Flow.CurrentDate</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Activate</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Re-Activate</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Program_Overview_Status_To_Activate</targetReference>
            </connector>
            <label>Update Status to Activate or Reactivate</label>
        </rules>
        <rules>
            <name>Update_Status_to_Expired_or_Terminated</name>
            <conditionLogic>3 AND (1 OR 2)</conditionLogic>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Expired</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Terminated</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentProgramOverview.LVGO__Program_End_Date__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Flow.CurrentDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Program_Overview_Status_to_Expired</targetReference>
            </connector>
            <label>Update Status to Expired or Terminated</label>
        </rules>
    </decisions>
    <decisions>
        <name>Checking_Whether_Active_CO_records_Found_or_Not</name>
        <label>Checking Whether Active CO records Found or Not</label>
        <locationX>1181</locationX>
        <locationY>157</locationY>
