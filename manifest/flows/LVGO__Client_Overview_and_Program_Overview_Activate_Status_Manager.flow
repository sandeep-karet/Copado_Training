                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Program_Overview_to_Active</targetReference>
            </connector>
            <label>Found Record</label>
        </rules>
    </decisions>
    <decisions>
        <name>Determine_Record_Type_and_Status</name>
        <label>Determine Record Type and Status</label>
        <locationX>727</locationX>
        <locationY>417</locationY>
        <defaultConnectorLabel>N/A</defaultConnectorLabel>
        <rules>
            <name>Client_Overview_Active</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>currentContractRecord.RecordTypeId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>clientOverviewRecordTypeId</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Activate</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Re-Activate</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Make_the_CO_status_as_Active</targetReference>
            </connector>
            <label>Client Overview - Activate</label>
        </rules>
        <rules>
            <name>Client_Overview_Expired</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>currentContractRecord.RecordTypeId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>clientOverviewRecordTypeId</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Expired</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Terminated</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Related_Program_Overviews</targetReference>
            </connector>
            <label>Client Overview - Expired</label>
        </rules>
        <rules>
            <name>Program_Overview_Active</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>currentContractRecord.RecordTypeId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>programOverviewRecordTypeId</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Activate</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentContractRecord.LVGO__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Re-Activate</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Related_Active_CO_records</targetReference>
            </connector>
            <label>Program Overview - Activate</label>
        </rules>
    </decisions>
