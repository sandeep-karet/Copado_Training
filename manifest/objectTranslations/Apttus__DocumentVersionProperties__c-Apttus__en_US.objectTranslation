        <label><!-- Document Regenerate --></label>
        <name>Apttus__DocumentRegenerate__c</name>
    </fields>
    <fields>
        <help><!-- Checking this box will trim the last decimal point and number from the document version in the naming convention. For example version 1.2.0 would become 1.2 in the name of the document. --></help>
        <label><!-- Trim Document Version Number --></label>
        <name>Apttus__DocumentVersionTrim__c</name>
    </fields>
    <fields>
        <help><!-- This property will determine the increment factor when a document is checked in as Final.  Acceptable values for this field are &quot;Major&quot; or &quot;Minor&quot;.  The default value is Major --></help>
        <label><!-- Increment Checkin Final --></label>
        <name>Apttus__IncrementCheckinFinal__c</name>
    </fields>
    <fields>
        <help><!-- This property will determine the increment factor when a document is checked in with redlines.  Acceptable values for this field are &quot;Major&quot; or &quot;Minor&quot;.  The default value is Minor --></help>
        <label><!-- Increment Checkin With Redlines --></label>
        <name>Apttus__IncrementCheckinWithRedlines__c</name>
    </fields>
    <fields>
        <help><!-- This property will determine the increment factor when a document is checked in without redlines.  Acceptable values for this field are &quot;Major&quot; or &quot;Minor&quot;.  The default value is Major --></help>
        <label><!-- Increment Checkin Without Redlines --></label>
        <name>Apttus__IncrementCheckinWithoutRedlines__c</name>
    </fields>
    <fields>
        <help><!-- This property will set the initial version number of a newly generated or uploaded document.  Acceptable values for this field are 1 or 0.  Default value is 1 --></help>
        <label><!-- Initial Version --></label>
        <name>Apttus__InitialVersion__c</name>
    </fields>
